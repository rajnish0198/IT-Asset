 "html": "<!DOCTYPE html>\n<body>\n    <style>\n        body{border:5px double black;}\n    </style>\n    \n<html>\n\n    <div class=\"text-center\" style=\"margin-bottom: 5%;colour:blue\"><h3>Asset Isssue Note</h3></div>\n\n    <!--<center><img src=\"https://enggrecruit.com/wp-content/uploads/2018/08/MSEDCL-LOGO3png.png\"  width=\"150\" height=\"100\"></center>-->\n    <!--<center><img src=\"/home/Music/datamann.png\"  width=\"150\" height=\"100\"></center>-->\n\n  <!--<center><h1 style=\"color:blue;\">Company Issue Asset To Employee .</h1></center>-->\n    <center><h1 style=\"color:black;\">{{doc.company}}</h1></center>\n\n  \n<hr size=\"5\" width=\"130%\" color=\"black\">\n<br>\n\n  <!--<center><h1 style=\"color:black;\">{{doc.company}}</h1></center>-->\n  \n  \n<!--<hr size=\"5\" width=\"130%\" color=\"black\">-->\n\n<div class ='row' style=\"margin-bottom: 2%;\">\n    <div class = \"col-sm-6\">\n        <b> Company:</b>\n            {{doc.company}} <br> <br>\n            \n        <b>Employee Name:</b>\n            {{doc.employee_name}} <br> <br>\n            \n    </div>\n    \n    <div class=\"col-sm-6\">\n        <span style = \"float: left;\"><b>Emp Id: </b>{{ doc.emp_id }}</span>\n    </div>\n    \n    \n    <div class = \"col-sm-6\">\n        <br><span style = \"float: left;\"><b>Name: </b>{{ doc.name1 }}</span>\n    </div>\n    \n    <div class = \"col-sm-7\">\n        <b> Asset Source Location:</b>\n            {{doc.asset_source_location}} <br> <br>\n            \n        <b>Asset Target Location:</b>\n            {{doc.asset_target_location}} <br> <br>\n            \n       \n            \n    </div>\n   \n    <!--<div class= \"col-sm-6\">-->\n    <!--    <b>Incentive up to Date :</b>-->\n    <!--        {{doc.incentive_up_to_date}}<br><br>-->\n            \n    <!--    <b>Payment Amt after Incentive Date:</b>-->\n    <!--        {{doc.payment_amt}} <br> <br>-->\n    <!--     <div>   -->\n    <!--    <b>Connection date :</b>-->\n    <!--        {{doc.connection_date}} <br> <br>-->\n    <!--</div>-->\n    \n    <!--    <div>-->\n    <!--        <b>Scan this QR code for UPI Payment:</b>-->\n    <!--            {{doc.scan_this_qr_code_for_upi_payment}}<br> <br>-->\n    <!--</div>-->\n    <hr size=\"5\" width=\"230%\" color=\"black\">\n                <b>Asset Take:</b>\n\n     <table style=\"width:100%\">\n         \n  <tr>\n    <th>Item Name</th>\n    <th>Asset Tag</th>\n    <th>Asset Categoary</th>\n    <th>Model No.</th>\n    <th>Qty Given</th>\n    <th>Deploy Date</th>\n    <th>Asset Return Date</th>\n\n\n\n\n  </tr>\n  <tr>\n     {% for ry in doc.asset_take %} \n  <tr style=\"font-size:15px;\">\n    <td>{{ ry.item_name }}</td>\n    <td>{{ ry.asset_tag }}</td>\n    <td>{{ry.asset_categoary }}</td>\n    <td>{{ry.model_no }}</td>\n    <td>{{ry.qty_given }}</td>\n    <td>{{ry.deploy_date }}</td>\n    <td>{{ry.asset_return_date }}</td>\n\n  </tr>\n  {% endfor %}\n  <tr>\n      </table>\n      <hr size=\"5\" width=\"130%\" color=\"black\">\n\n     \n<!--    <hr size=\"5\" width=\"230%\" color=\"black\">-->\n    \n<!--    <div class ='row' style=\"margin-bottom: -7%;font-family:bellmt;font-size:17px\">-->\n<!--    <div class = \"col-sm-6\"><b>Per Unit Cost:</b>-->\n<!--        {{ doc.per_unit_cost}}-->\n<!--    </div>-->\n<!--    <div class=\"col-sm-6\" style=\"font-family:bellmt;font-size:17px\">-->\n<!--        <span style = \"float: right;\"><b></b>{{ doc.consumer_type }}</span>-->\n<!--    </div>-->\n<!--</div>-->\n     \n    \n    \n    <div  style = \"float: left; padding-top:150px; padding-left:13px\">\n        <span><b>Signature<br> </b>{{\"IT HEAD\" }}</span>\n    </div>\n</div>\n\n<div class ='row' style=\"margin-bottom: 2%;\">\n    <div class = \"col-sm-6\">\n         Date:    <small>{{frappe.utils.now()}}</small>\n</div>\n</div>\n",
